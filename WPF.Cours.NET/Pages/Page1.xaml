<Page x:Class="WPF.Cours.NET.Pages.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF.Cours.NET.Pages"
    xmlns:model="clr-namespace:WPF.Cours.NET.Models"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">
    <Grid>
        <Button Command="{Binding Path=(model:PageModel.Instance).GoTo}" CommandParameter="OTHER" Height="30" VerticalAlignment="Center" HorizontalAlignment="Center" Width="200" Content="Start"/>
    </Grid>
</Page>
